{
    "version": 2,
    "builds": [
        {
            "src":"src/api.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"(.*)",
            "dest":"src/api.js",
            "methods": ["GET", "POST", "OPTIONS", "PUT", "DELETE", "PATCH"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}